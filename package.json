{
  "name": "backend-static-testing-setup",
  "version": "1.0.0",
  "description": "Static testing like eslint, prettier etc.",
  "scripts": {
    "test": "jest",
    "build": "rm -rf ./dist && babel src --out-dir dist --ignore **/*.test.js",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore --write \"src**/*.+(js|json)\"",
    "format": "npm run lint -- --fix",
    "validate": "npm run lint && npm test && npm run build",
    "prepare": "husky install",
    "start": "npm run build && NODE_ENV=production node ./dist/index.js",
    "start:local": "NODE_ENV=local nodemon ./src/start.js"
  },
  "author": "jatin parate <jatin.parate@mlveda.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jest": "^27.2.5",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.13",
    "prettier": "^2.3.2"
  },
  "dependencies": {
    "@babel/core": "^7.15.4",
    "@babel/preset-env": "^7.15.4",
    "@babel/register": "^7.15.3",
    "core-js": "^3.17.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "http-errors": "^1.7.2",
    "log4js": "^6.3.0",
    "regenerator-runtime": "^0.13.9"
  }
}
